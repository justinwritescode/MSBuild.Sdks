<Project Sdk="Microsoft.Build.NoTargets">
   <UsingTask TaskName="JustinWritesCode.MSBuild.UsingsSdk.CreateUsingsProject" AssemblyFile="$(UsingsSdkAssemblyFile)" />
   <Target Name="PrepareOutputFile" BeforeTargets="GetPackageContents">
      <JustinWritesCode.MSBuild.UsingsSdk.CreateUsingsProject
            InputFile="$(MSBuildProjectFullPath)"
            OutputFile="$(OutputFilePath)" />
   </Target>
   <ItemGroup>
    <Content Include="$(OutputFilePath)" PackagePath="$(OutputFilePackagePath)" />
    <Content Include="$(IconFile)" PackagePath="" />
   </ItemGroup>
   <Target Name="OutputProperties" AfterTargets="Build">
      <Message Text="PackageIdOverride: $(PackageIdOverride)" Importance="High" />
      <Message Text="AssemblyNameOverride: $(AssemblyNameOverride)" Importance="High" />
      <Message Text="RootNamespaceOverride: $(RootNamespaceOverride)" Importance="High" />
      <Message Text="PackageId: $(PackageId)" Importance="High" />
      <Message Text="RootNamespace: $(RootNamespace)" Importance="High" />
      <Message Text="AssemblyName: $(AssemblyName)" Importance="High" />
      <Message Text="OutputFileExtension: $(OutputFileExtension)" Importance="High" />
      <Message Text="OutputFilePackagePath: $(OutputFilePackagePath)" Importance="High" />
      <Message Text="OutputFilePath: $(OutputFilePath)" Importance="High" />
   </Target>
</Project>
